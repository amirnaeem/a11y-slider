{
  "name": "a11y-slider",
  "version": "0.0.2",
  "description": "Library for accessible sliders",
  "scripts": {
    "dev": "npm run rollup:watch",
    "build": "npm run rollup:build && npm run build:types",
    "rollup:build": "rollup -c",
    "rollup:watch": "rollup -c -w",
    "build:types": "tsc --emitDeclarationOnly",
    "prebuild": "rimraf dist",
    "prepublishOnly": "npm run build && git add * && git commit -m \"Updating build\"",
    "release": "np patch --any-branch"
  },
  "main": "dist/a11y-slider.cjs.js",
  "module": "dist/a11y-slider.esm.js",
  "browser": "dist/a11y-slider.js",
  "unpkg": "dist/a11y-slider.js",
  "style": "dist/a11y-slider.css",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mmahandev/a11y-slider.git"
  },
  "bugs": {
    "url": "https://github.com/mmahandev/a11y-slider/issues"
  },
  "author": "mmahandev",
  "homepage": "https://github.com/mmahandev/a11y-slider",
  "keywords": [
    "accessible",
    "slider",
    "carousel",
    "javascript",
    "typescript",
    "library",
    "es6",
    "js",
    "accessibility",
    "a11y"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-typescript": "^7.6.3",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "core-js": "^3.2.1",
    "np": "^5.1.1",
    "rimraf": "^3.0.0",
    "rollup": "^1.23.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-livereload": "^1.0.4",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-terser": "^5.1.2",
    "ts-debounce": "^1.0.0",
    "tslib": "^1.10.0",
    "typescript": "^3.6.4"
  },
  "dependencies": {}
}
